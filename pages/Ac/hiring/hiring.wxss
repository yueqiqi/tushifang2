/* 图片 */
.img_box{
  width:91%;
  position:relative;
  display: flex;
  flex-wrap: wrap;
  margin:0 auto;
}
.imgsz{
  display: flex;
  justify-content: center;
  margin-bottom:20rpx;
  object-fit:cover
}
/* 添加图片的样式 */
.imgsz .im{
  width:200rpx;
  height:200rpx;
  object-fit:cover;
  border:1px solid rgba(214, 212, 212, 0.1);
  margin-top:20rpx;
}

.imgs .images{
  position:relative;
}
.images button{
  width:100%;
  height:100%;
  position:absolute;
  top:0;
  left:0;
}
/* 添加图片按钮 */
.img_box .images{
  margin-top:20rpx;
  width:90%;
	height: 168rpx;
  border:4px dashed  #E8E8E8;
  border-radius:4rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}
.img_box .images>image{
  width:60rpx;
	height:60rpx;
}

