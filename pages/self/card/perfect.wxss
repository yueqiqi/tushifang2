/* pages/self/card/perfect.wxss */
@import '../err/err.wxss';
page{
  background: #fff!important;
}
/* pages/self/card/card.wxss */
.card{
  width:690rpx;
height:410rpx;
background:rgba(255,255,255,1);
box-shadow:0rpx 0rpx 10rpx 0rpx rgba(217,217,217,1);
border-radius:10rpx;
margin:0 auto;
padding-top:31rpx;
}
.head{
  width:140rpx;
height:140rpx;
border-radius:50%;
}
/* 用户昵称 */
.me{
  margin-left:29rpx;
}
.rights{
  margin-left:30rpx;
}
.name{
  font-size:36rpx;
font-family:PingFang SC;
font-weight:bold;
color:rgba(51,51,51,1);
margin-left:30rpx;padding-top:30rpx;
}
.me1{
  font-size:28rpx;
font-family:PingFang SC;
font-weight:400;
color:rgba(51,51,51,1);
}
/*  */
.ico{
  width:39rpx;height:31rpx;
}
.mle{
  margin-left:19rpx;
}
/* 按钮 */

/* 分享标题 */
.st{
  font-size:30rpx;
font-family:PingFang SC;
font-weight:bold;
color:rgba(51,51,51,1);
margin-left:50rpx;
margin-bottom:10rpx;
}
.i1{
  width:690rpx;
height:60rpx;
background:rgba(235,249,247,1);
border-radius:10rpx;
margin:0 auto;
}
/* 下拉选项框 */
.select_box{
  width:690rpx;
  height:70%;
  position: relative;z-index:50000;margin-left:30rpx;
}
/* 自定义列表 */
.select_box .option_box{
  position: absolute;
  top: calc(100% - 1px);
  width: 690rpx;
  /* height:404rpx; */
  box-sizing: border-box;
  /* height: 0; */
  overflow-y: auto;
  background: #fff;
  transition: height 0.3s;
  border-left:1px solid #efefef;
  border-right:1px solid #efefef;
  background:rgba(235,249,247,1);
 
/* border-radius:20rpx; */

}
/* 自定义列表文字 */
.select_box .option_box .option{
  display: block;
  line-height: 30rpx; 
  /* border-top: 1px solid #efefef;*/
  border-bottom: 2rpx solid rgba(255,255,255,1); 
  padding: 10rpx;padding-top:28rpx;padding-left:30rpx;
     font-size:30rpx;
font-family:PingFang SC;
font-weight:400;
color:rgba(72,149,137,1);
  
}
/* 下拉图片 */
.sel{
  width:29rpx;height:17rpx;
  margin:20rpx 20rpx 21rpx 0rpx;
}
.i1 input{
  width:600rpx;
  margin-left:19rpx;
  margin-top:8rpx;
  font-size:30rpx;
font-family:PingFang SC;
font-weight:400;
color:rgba(72,149,137,1);
}
/* 基本信息 */
.til{
  font-size:40rpx;
font-family:PingFang SC;
font-weight:bold;
color:rgba(51,51,51,1);
}
.section{
  margin-top:35rpx;
}
.section-title{
  font-size:30rpx;
font-family:PingFang SC;
font-weight:400;
color:rgba(51,51,51,1);
}
.right{
  margin-right:40rpx;
  width:510rpx;
}
/* 下拉图片 */
.section-img{
  width:17rpx;height:29rpx;
}
.right{
  border-bottom:2rpx solid rgba(217,217,217,1);
  /* padding-bottom:17rpx; */
}
/* 选择头像 */
.header{
  width:90rpx;
height:90rpx;
border-radius:50%;
margin-top:-20rpx;
}
/* 姓名 */
.section2{
margin-top:53rpx;
}
/* 更换 */
.ch{
  width:100rpx;
height:50rpx;
background:rgba(235,249,247,1);
border-radius:10rpx;
text-align: center;
line-height: 50rpx;
font-size:28rpx;
font-family:PingFang SC;
font-weight:500;
color:rgba(72,149,137,1);
margin-bottom:20rpx;
}
.s1{
  margin-top:40rpx;
}
/* 个人简介 */
.self{
  margin-top:58rpx;
}
/* 个人简介文本框 */
.texta{
  margin-top:19rpx;
  width:650rpx;
height:230rpx;
border:1px solid rgba(217,217,217,1);
border-radius:10rpx;
padding-top:20rpx;padding-left:20rpx;
}
textarea{
  width:650rpx;
height:228rpx;
}

/* 上传图片 */
.imgss{
  margin-top:20rpx;
}
/* 营业执照 */
.license{
  font-size:30rpx;
font-family:PingFang SC;
font-weight:400;
color:rgba(51,51,51,1);
}
.onload{
  margin-top:19rpx;
  width:130rpx;
height:130rpx;
background:rgba(235,249,247,1);
border-radius:7rpx;
/* text-align: center; */
font-size:24rpx;
font-family:PingFang SC;
font-weight:400;
color:rgba(72,149,137,1);
/* 让元素水平垂直居中 */
display: flex;
  flex-direction: column;
  align-items: center;
   justify-content: center;

}
/* 图片 */
.onload image{
  width:40rpx;height:40rpx;
  margin-bottom:10rpx;
}
/* /法人证明书 */
.prove{
  margin-top:41rpx;
}
.license text{
  color:#999999;
  font-size:24rpx;
}


/* 图片 */
.img_box{
  width:91%;
  position:relative;
  display: flex;
  flex-wrap: wrap;
  margin:0 auto;
}
.imgs{
  width:33.33333333%;
  display: flex;
  justify-content: center;
  margin-bottom:20rpx;
}
/* 添加图片的样式 */
.imgs image{
  /* width:100%; */
  width:168rpx;
  height:168rpx;
  /* max-height:212rpx; */
  border:1px solid rgba(214, 212, 212, 0.1);
  /* box-shadow: 5rpx 5rpx 1rpx 3rpx #e2e0e0; */
  margin-top:20rpx;
}

.imgs .images{
  position:relative;
}
.images button{
  width:100%;
  height:100%;
  position:absolute;
  top:0;
  left:0;
}
/* 添加图片按钮 */
.img_box .images{
  margin-top:20rpx;
  width:90%;
	height: 168rpx;
  border:4px dashed  #E8E8E8;
  border-radius:4rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}
.img_box .images>image{
  width:60rpx;
	height:60rpx;
}
/* 确认保存 */
.next{
  width:650rpx;
height:70rpx;
background:rgba(118,212,198,1);
border-radius:10rpx;
font-size:30rpx;
font-family:PingFang SC;
font-weight:500;
color:rgba(255,255,255,1);
text-align: center;
line-height: 70rpx;
margin:0 auto;
margin-top:61rpx;
margin-left:-2rpx;
margin-bottom:15rpx;
}