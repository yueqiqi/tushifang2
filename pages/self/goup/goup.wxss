/* pages/self/goup/goup.wxss */
page{
  height:100%!important;
}
.tit{
  font-size:30rpx;
font-family:PingFang SC;
font-weight:bold;
color:rgba(118,212,198,1);
}
.sco{
  font-size:30rpx;
font-family:PingFang SC;
font-weight:bold;
color:rgba(51,51,51,1);
margin-left:31rpx;
}
.right{
  margin-left:133rpx;
}
/* 选择样式 */
.btns{
  width:220rpx;
height:130rpx;
background:rgba(255,255,255,1);
border:2rpx solid rgba(118,212,198,1);
border-radius:10rpx;
text-align: center;
}
/* 按钮文字样式 */
.btns :first-child{
font-size:36rpx;
font-family:PingFang SC;
font-weight:bold;
color:rgba(51,51,51,1);
padding-top:15rpx;
}
.btns :last-child{
  font-size:30rpx;
font-family:PingFang SC;
font-weight:400;
color:rgba(153,153,153,1);
}
.next{
 width:100%;
height:98rpx;
background:rgba(118,212,198,1);
border-radius:10rpx;
font-size:36rpx;
font-family:PingFang SC;
font-weight:500;
color:rgba(255,255,255,1);
text-align: center;
line-height: 98rpx;
position: fixed;
bottom:0;
}
.active{
  background:#76D4C6;
}
.active :first-child{
    color:#fff;
}
.active :last-child{
    color:#fff;
}
.in{
  flex-wrap: wrap;
}